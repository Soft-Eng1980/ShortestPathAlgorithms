            <Grid.RowDefinitions>
                <RowDefinition Height="130" />
                <RowDefinition Height="*" />
                <RowDefinition Height="120" />
            </Grid.RowDefinitions>
            <Label
                x:Name="lbl_Inception"
                Width="93"
                Margin="4,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Inception Point" />
            <Label
                x:Name="lbl_Destination"
                Width="105"
                Margin="116,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Destination Point" />
            <ComboBox
                x:Name="Destination_Point"
                Width="107"
                Height="21"
                Margin="115,41,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top" />
            <ComboBox
                x:Name="Inception_Point"
                Width="107"
                Height="21"
                Margin="3,41,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top" />
            <Button
                x:Name="CalculateButton"
                Width="80"
                Height="41"
                Margin="32,74,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="OnCalculateButtonClick"
                Content="Calculate"
                IsEnabled="False" />

            <Button
                x:Name="RandomDataButton"
                Width="80"
                Height="41"
                Margin="124,74,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="OnFakeDataButtonClick"
                Content="Random Data" />

            <ScrollViewer
                x:Name="PanelScrollViewer"
                Grid.Row="1"
                Margin="5">
                <StackPanel
                    x:Name="PanelStackPanel"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Orientation="Vertical" />
            </ScrollViewer>


            <StackPanel
                Grid.Row="2"
                Margin="3"
                Orientation="Vertical">
                <Button
                    x:Name="btn_Lecturer"
                    Grid.Row="2"
                    Width="198"
                    Height="27"
                    Margin="5"
                    HorizontalAlignment="Center"
                    Click="OnLecturerButtonClick"
                    Content="Lecturer" />
                <Button
                    x:Name="btn_Creators"
