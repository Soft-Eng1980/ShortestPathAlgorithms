<Window
    x:Class="Dijkstra.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="TSP"
    Title="TSP"
    Width="1200"
    Height="970"
    Loaded="Load"
    ResizeMode="NoResize"
    mc:Ignorable="d">
    <Grid x:Name="T_S_P" Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Grid x:Name="InputPanel" Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="130" />
                <RowDefinition Height="*" />
                <RowDefinition Height="120" />
            </Grid.RowDefinitions>
            <Label
                x:Name="lbl_Inception"
                Width="93"
                Margin="4,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Inception Point" />
            <Label
                x:Name="lbl_Destination"
                Width="105"
                Margin="116,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Destination Point" />
            <ComboBox
                x:Name="Inception_Point_Copy"
                Width="107"
                Height="21"
                Margin="115,41,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top" />
            <ComboBox
                x:Name="Inception_Point"
                Width="107"
                Height="21"
                Margin="3,41,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top" />
            <Button
                x:Name="btn_calc"
                Width="154"
                Height="41"
                Margin="32,74,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="btn_calc_Click"
                Content="Calculate"
                IsEnabled="False"
                RenderTransformOrigin="0.5,0.5">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="1" ScaleY="1" />
                        <SkewTransform AngleX="0" AngleY="0" />
                        <RotateTransform Angle="0" />
                        <TranslateTransform />
                    </TransformGroup>
                </Button.RenderTransform>
            </Button>

            <ScrollViewer
                x:Name="PanelScrollViewer"
                Grid.Row="1"
                Margin="5">
                <StackPanel
                    x:Name="PanelStackPanel"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Orientation="Vertical" />
            </ScrollViewer>


            <StackPanel
                Grid.Row="2"
                Margin="3"
                Orientation="Vertical">
                <Button
                    x:Name="btn_Lecturer"
                    Grid.Row="2"
                    Width="198"
                    Height="27"
                    Margin="5"
                    HorizontalAlignment="Center"
                    Click="btn_Lecturer_Click"
                    Content="Lecturer" />
                <Button
                    x:Name="btn_Creators"
                    Grid.Row="2"
                    Width="198"
                    Height="27"
                    Margin="5"
                    HorizontalAlignment="Center"
                    Click="btn_Creators_Click"
                    Content="Creators" />
                <Button
                    x:Name="btn_Course"
                    Grid.Row="2"
                    Width="198"
                    Height="27"
                    Margin="5"
                    HorizontalAlignment="Center"
                    Click="btn_Course_Click"
                    Content="Course" />
            </StackPanel>

        </Grid>




