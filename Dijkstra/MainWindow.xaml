<Window
    x:Class="Dijkstra.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cc="clr-namespace:Dijkstra.CustomControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="TSP"
    Title="TSP"
    Width="1300"
    Height="995"
    AllowsTransparency="True"
    Background="Transparent"
    FontFamily="FiraCode Nerd Font Mono Ret"
    FontSize="11"
    Loaded="Load"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Grid x:Name="T_S_P" Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Border
            Grid.Column="0"
            Margin="20"
            Background="GhostWhite"
            BorderBrush="LightSlateGray"
            BorderThickness="1"
            CornerRadius="5">
            <Border.Effect>
                <DropShadowEffect
                    BlurRadius="10"
                    Direction="270"
                    Opacity="0.6"
                    RenderingBias="Quality"
                    Color="Gray" />
            </Border.Effect>

            <Grid x:Name="InputPanel">
                <Grid.RowDefinitions>
                    <RowDefinition Height="150" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="50" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="1.3*" />
                    </Grid.RowDefinitions>
                    <Border
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Background="LightGray"
                        CornerRadius="5,5,10,10"
                        MouseLeftButtonDown="OnBorderDraging">
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="25" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <Label
                                Grid.Column="0"
                                Width="20"
                                Padding="4,0,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Background="Transparent"
                                BorderThickness="0"
                                Content="+"
                                Cursor="Hand"
                                FontSize="18"
                                Foreground="Red"
                                MouseLeftButtonDown="OnCloseLableClick"
                                RenderTransformOrigin="0.5,0.5">
                                <Label.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform />
                                        <SkewTransform />
                                        <RotateTransform Angle="45" />
                                        <TranslateTransform />
                                    </TransformGroup>
                                </Label.RenderTransform>
                            </Label>

                            <Label
                                Grid.Column="1"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Content="Find Shortest Path Algorithms"
                                Foreground="DodgerBlue" />
                        </Grid>
                    </Border>

                    <Label
                        x:Name="lbl_Inception"
                        Grid.Row="1"
                        Grid.Column="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Inception Point" />
                    <Label
                        x:Name="lbl_Destination"
                        Grid.Row="1"
                        Grid.Column="1"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Destination Point" />
                    <ComboBox
                        x:Name="Destination_Point"
                        Grid.Row="2"
                        Grid.Column="0"
                        Width="107"
                        Height="21"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center" />
                    <ComboBox
                        x:Name="Inception_Point"
                        Grid.Row="2"
                        Grid.Column="1"
                        Width="107"
                        Height="21"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center" />
                    <StackPanel
                        Grid.Row="3"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Horizontal">
                        <Button
                            x:Name="CalculateButton"
                            Width="80"
                            Height="41"
                            Margin="5,0"
                            Click="OnCalculateButtonClick"
                            Content="Calculate"
                            IsEnabled="False" />

                        <Button
                            x:Name="RandomDataButton"
                            Width="80"
                            Height="41"
                            Margin="5,0"
                            Click="OnFakeDataButtonClick"
                            Content="Random Data" />
                    </StackPanel>
                </Grid>


                <ScrollViewer
                    x:Name="PanelScrollViewer"
                    Grid.Row="1"
                    Margin="10">
                    <StackPanel
                        x:Name="PanelStackPanel"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Orientation="Vertical" />
                </ScrollViewer>

                <Button
                    x:Name="btn_Creators"
                    Grid.Row="2"
                    Width="198"
                    Height="27"
                    Margin="5"
                    HorizontalAlignment="Center"
                    Click="OnCreatorsButtonClick"
                    Content="Presented By" />

            </Grid>
        </Border>





